{
  "name": "@dotsetlabs/axion",
  "version": "1.0.0",
  "description": "Secrets SDK for the dotset platform. Zero-disk encrypted secrets with AES-256-GCM.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./sdk": {
      "types": "./dist/sdk.d.ts",
      "import": "./dist/sdk.js"
    },
    "./crypto": {
      "types": "./dist/crypto.d.ts",
      "import": "./dist/crypto.js"
    },
    "./manifest": {
      "types": "./dist/manifest.d.ts",
      "import": "./dist/manifest.js"
    },
    "./injector": {
      "types": "./dist/injector.d.ts",
      "import": "./dist/injector.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./parser": {
      "types": "./dist/parser.d.ts",
      "import": "./dist/parser.js"
    },
    "./templates": {
      "types": "./dist/templates.d.ts",
      "import": "./dist/templates.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "dotset",
    "secrets",
    "encryption",
    "environment",
    "env",
    "security",
    "sdk",
    "library"
  ],
  "author": "dotset labs <hello@dotsetlabs.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotsetlabs/axion.git"
  },
  "bugs": {
    "url": "https://github.com/dotsetlabs/axion/issues"
  },
  "homepage": "https://dotsetlabs.com",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "argon2": "^0.44.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}